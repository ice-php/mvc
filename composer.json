{
  "name": "ice-php/mvc",
  "description": "控制器,模型,视图相关",
  "keywords": [
    "icePHP",
    "template",
    "view",
    "model",
    "controller",
    "request",
    "action"
  ],
  "homepage": "https://github.com/ice-php/mvc",
  "type": "composer",
  "license": "MIT",
  "support": {
    "source": "https://github.com/ice-php/mvc"
  },
  "authors": [
    {
      "name": "blueice",
      "email": "liyong@nenu.edu.cn"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ice-php/write": "dev-master",
    "ice-php/cache": "dev-master",
    "ice-php/config": "dev-master",
    "ice-php/functions": "dev-master"
  },
  "minimum-stability": "dev",
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "icePHP\\": "src"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  },
  "config": {
    "process-timeout": 1800
  }
}
